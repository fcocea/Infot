{
  "compilerOptions": {
    "target": "ES2020",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/typegram": [
        "node_modules/.pnpm/telegraf@4.12.2/node_modules/telegraf/typings/core/types/typegram"
      ],
      "@/utils/*": ["src/utils/*"],
      "@/utils": ["src/utils"],
      "@/commands/*": ["src/commands/*"],
      "@/commands": ["src/commands"],
      "@/middlewares": ["src/middlewares"],
      "@/middlewares/*": ["src/middlewares/*"]
    },
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
