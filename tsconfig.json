{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/typegram": [
        "node_modules/.pnpm/telegraf@4.12.2/node_modules/telegraf/typings/core/types/typegram"
      ],
      "@/utils/*": ["src/utils/*"],
      "@/utils": ["src/utils"],
      "@/commands/*": ["src/commands/*"],
      "@/commands": ["src/commands"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
